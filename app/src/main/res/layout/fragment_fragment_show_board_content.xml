    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KarteAnzeigen.KarteAnzeige">

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/fragment_fragment_show_board_content_ViewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <esaph.filing.Utils.LoadingState.LoadingStateHandler
        android:id="@+id/fragment_fragment_show_board_content_anzeige_LoadingStateHandler"
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>